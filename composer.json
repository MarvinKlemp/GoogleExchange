{
    "name": "marvinklemp/googleexchange",
    "description": "An adapter for marvinklemp/money to google exchange",
    "license": "MIT",
    "authors": [
        {
            "name": "Marvin Klemp",
            "email": "mail@marvin-klemp.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "marvinklemp/money": "1.0.0-alpha1",
        "guzzlehttp/guzzle": "5.3.0",
        "symfony/dom-crawler": "~3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "MarvinKlemp\\GoogleExchange\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarvinKlemp\\GoogleExchange\\Tests\\": "tests/"
        }
    }
}
